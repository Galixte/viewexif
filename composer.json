{
    "name": "canonknipser/viewexif",
    "type": "phpbb-extension",
    "description": "Show exif-data for images posted as attachment for phpBB 3.1",
    "homepage": "https://canonknipser.com",
    "version": "1.0.1",
    "time": "2016-11-04",
    "license": "GPL-2.0",
    "authors": [{
            "name": "canonknipser",
            "email": "siekmannfrank@gmail.com",
            "homepage": "https://forum.shutter-friends.de",
            "role": "Lead Developer"
        }],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Show exif-data",
        "soft-require": {
            "phpbb/phpbb": "~3.1"
        },
        "version-check": {
            "host": "download.canonknipser.com",
            "directory": "/v-check",
            "filename": "viewexif_version.json"
        }
    }
}
